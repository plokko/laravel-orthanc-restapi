{
    "name": "plokko/laravel-orthanc-restapi",
    "description": "Laravel Orthanc REST API integration",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Plokko\\LaravelOrthancRestApi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Francesco Pellerone"
        }
    ],
    "require": {
        "php": ">=8.2",
        "laravel/framework": "^11.0|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Plokko\\LaravelOrthancRestApi\\LaravelOrthancRestApiServiceProvider"
            ],
            "aliases": {
                "OrthancApi": "Plokko\\LaravelOrthancRestApi\\Facades\\OrthancApi"
            }
        }
    }
}